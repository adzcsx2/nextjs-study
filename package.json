{
   "name": "next_app",
   "version": "0.1.0",
   "private": true,
   "scripts": {
      "dev": "npm run i18n_clean && concurrently \"next dev --turbopack\" \"node scripts/i18n-keyboard-listener.js\"",
      "build": "npm run i18n_clean && next build --turbopack",
      "build:test": "cross-env NODE_ENV=production npm run i18n_clean && cross-env NODE_ENV=production next build --turbopack",
      "build:production": "cross-env NODE_ENV=production npm run i18n_clean && cross-env NODE_ENV=production next build --turbopack",
      "start": "next start",
      "start:test": "cross-env NODE_ENV=production next start -p 3001",
      "start:production": "cross-env NODE_ENV=production next start -p 3000",
      "lint": "eslint",
      "i18n_clean": "node scripts/i18n-keyboard-listener.js --cleanup"
   },
   "dependencies": {
      "@ant-design/icons": "^5.6.1",
      "@ant-design/nextjs-registry": "^1.1.0",
      "@ant-design/v5-patch-for-react-19": "^1.0.3",
      "antd": "^5.27.4",
      "i18next": "^25.5.2",
      "i18next-browser-languagedetector": "^8.2.0",
      "next": "15.5.3",
      "react": "19.1.0",
      "react-dom": "19.1.0",
      "react-i18next": "^15.7.3",
      "zustand": "^5.0.8"
   },
   "devDependencies": {
      "@eslint/eslintrc": "^3",
      "@tailwindcss/postcss": "^4",
      "@tenado/i18n-cli": "^1.4.9",
      "@types/node": "^20",
      "@types/react": "^19",
      "@types/react-dom": "^19",
      "concurrently": "^9.2.1",
      "cross-env": "^10.0.0",
      "eslint": "^9",
      "eslint-config-next": "15.5.3",
      "tailwindcss": "^4",
      "typescript": "^5"
   }
}
